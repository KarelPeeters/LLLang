//before
fun %main() {
  <entry>
    %c i1 = blur 1 i1
    branch %c i1 <then> <else>
  <then>
    jump <end>
  <else>
    jump <end>
  <end>
    %x i32 = phi[<then>: 2 i32, <else>: undef i32]
    eat(%x i32)
    return unit Unit
}

//after
fun %main() {
  <entry>
    %c i1 = blur 1 i1
    branch %c i1 <then> <else>
  <then>
    jump <end>
  <else>
    jump <end>
  <end>
    eat(2 i32)
    return unit Unit
}

