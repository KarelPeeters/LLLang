//before
fun %main() {
  <entry>
    jump <while.cond>
  <while.cond>
    %x i32 = phi [<entry>: 5 i32, <while.body>: %x1 i32]
    branch 1 i1 <while.body> <while.end>
  <while.body>
    %x1 i32 = call %replace(%x i32)
    jump <while.cond>
  <while.end>
    eat(%x i32)
