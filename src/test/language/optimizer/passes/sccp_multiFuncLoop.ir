fun %replace(%x i32): i32 {
  <entry>
    %c i1 = neq 5 i32, 5 i32
    branch 0 i1 <then> <else>
  <then>
    jump <end>
  <else>
    jump <end>
  <end>
    %r i32 = phi [<then>: 6 i32, <else>: 5 i32]
    return 5 i32
}
